<?php
require_once('const_def_qygl.php');
require_once(APPLICATION_PATH."/jqgrid/qygl/yw_tool.php");

$yw_tool = new yw_tool($this->tool);
$ret = $yw_tool->getTemplate($this->yw_fl_id);
